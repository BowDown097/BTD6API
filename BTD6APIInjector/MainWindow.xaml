<Window x:Class="BTD6APIInjector.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="BTD6API Injector" Height="83.621" Width="auto"
        SizeToContent="WidthAndHeight">
    <Grid>
        <DockPanel HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,10,10,10">
            <Button Content="Import Mod" DockPanel.Dock="Left" Click="Import_Click"/>
            <Button Content="?" Width="20" Click="Help_Click"/>
            <Label Name="runningStatus" Foreground="Red" Content="BTD6 is not running"/>
        </DockPanel>
        <StackPanel HorizontalAlignment="Left" Height="Auto" Margin="10,30,0,0" VerticalAlignment="Top" Width="auto" Name="modList"/>
    </Grid>
</Window>
